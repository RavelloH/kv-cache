{
    "rewrites": [{
        "source": "/",
        "destination": "/api/main"
    },
        {
            "source": "/set",
            "destination": "/api/main?mode=set"
        },
        {
            "source": "/get",
            "destination": "/api/main?mode=get"
        },
        {
            "source": "/del",
            "destination": "/api/main?mode=del"
        },
        {
            "source": "/(.*)",
            "destination": "/api/main"
        }],
    "headers": [{
        "source": "/(.*)",
        "headers": [{
            "key": "Access-Control-Allow-Origin",
            "value": "*"
        }]
    }]
}